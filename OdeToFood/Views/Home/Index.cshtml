@model OdeToFood.ViewModels.HomeIndexViewModel
@{
    ViewData["Title"] = "Index";
}

<table>
    @foreach (var item in Model.Resturants)
    {
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td><a asp-action="Details" asp-route-id="@item.Id">More</a></td>
    </tr>
    }
</table>
<a asp-action="Create">Add Resturant</a>


